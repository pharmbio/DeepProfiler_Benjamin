{
  "dataset": {
    "metadata": {
      "label_field": "Metadata_cmpdName",
      "control_value": "CBK281357"
    },
    "images": {
      "channels": [
        "DNA",
        "Mito",
        "AGP",
        "RNA",
        "ER"
      ],
      "file_format": "tiff",
      "bits": 16,
      "width": 3000,
      "height": 3000
    },
    "locations": {
      "mode": "single_cells",
      "box_size": 224,
      "area_coverage": 0.75,
      "mask_objects": false
    }
  },
  "prepare": {
    "illumination_correction": {
      "down_scale_factor": 8,
      "median_filter_size": 48
    },
    "compression": {
      "implement": false,
      "scaling_factor": 1.0
    }
  },
  "train": {
    "partition": {
      "targets": [
        "Metadata_cmpdName"
      ],
      "split_field": "Metadata_Split",
      "training": [
        1
      ],
      "validation": [
        0
      ]
    },
    "model": {
      "name": "efficientnet",
      "crop_generator": "sampled_crop_generator",
      "augmentations": true,
      "metrics": [
        "accuracy",
        "top_k",
        "average_class_precision"
      ],
      "epochs": 3,
      "initialization": "ImageNet",
      "params": {
        "label_smoothing": 0.0,
        "learning_rate": 0.005,
        "batch_size": 32,
        "conv_blocks": 0
      }
    },
    "sampling": {
      "factor": 1,
      "workers": 1
    },
    "validation": {
      "frequency": 1,
      "top_k": 5,
      "batch_size": 32,
      "frame": "val",
      "sample_first_crops": true
    }
  },
  "profile": {
    "feature_layer": "block6a_activation",
    "checkpoint": "Cell_Painting_CNN_v1.hdf5",
    "batch_size": 128
  }
}
